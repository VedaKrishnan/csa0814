import numpy as np
def matrix_sums(matrix):
mat =np array(matrix)
rows_sums=np.sum(mat,axis=1)
column_sum =np.sum(mat,axis=0)
main_diagonal_sum = np. sum(np.diag(math))
anti_diagonal_sum = np .sum(np.diag(np.fliplr(math)))

return rows_sums,column_sums,main_diagonal_sum,anti_diagonal_sum
matrix =[
  [1,2,3],
  [4,5,6],
  [7,8,9],
]
rows_sums,column_sums,main_diagonal_sum,anti_diagonal_sum=matrix_sums(matrix)
print("row sums:",rows_sums)
print("column sum:",column_sums)
print("main diagonal sum:",main_diagonal_sum)
print("anti diagonal sum:",anti_diagonal_sum)
